{
  "name": "vg-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "npm run test:unit && npm run test:e2e",
    "test:watch": "jest --watch",
    "test:cov": "npm run test:unit:cov && npm run test:e2e:cov",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:unit": "jest --config ./jest.config.json --testPathPatterns=src",
    "test:unit:cov": "jest --config ./jest.config.json --testPathPatterns=src --coverage --verbose",
    "test:e2e": "jest --config ./test/jest-e2e.json --verbose --runInBand --passWithNoTests",
    "test:e2e:cov": "jest --config ./test/jest-e2e.json --coverage --verbose --runInBand --passWithNoTests",
    "generate-api-client": "openapi-generator-cli generate -i ./generated/openapi/swagger.yaml -g typescript-axios -o ./generated/openapi/api-client --additional-properties=withSeparateModelsAndApi=true,modelPackage=models,apiPackage=api",
    "generate-supabase-types": "npx supabase gen types typescript --project-id yxgtxtejrtflqfcmzsgj > ./generated/supabase/database.types.ts",
    "start:redis": "docker run -d --name vg-backend-redis -p 6379:6379 redis:7",
    "pause:redis": "docker pause vg-backend-redis"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@openapitools/openapi-generator-cli": "^2.23.1",
    "@supabase/supabase-js": "^2.57.4",
    "axios": "^1.12.1",
    "redis": "^5.8.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.1.6",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supabase": "^2.40.7",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
